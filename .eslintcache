e that the user index stays within our range of users\n    if (userIndex >= users.length) {\n      userIndex = 0;\n    }\n    setUser(users[userIndex]);\n    setUserIndex(userIndex);\n  }\n\n  function previousUser(userIndex) {\n    // Ensure that the user index stays within our range of users\n    if (userIndex < 0) {\n      userIndex = users.length - 1;\n    }\n    setUser(users[userIndex]);\n    setUserIndex(userIndex);\n  }\n\n  function handleBtnClick(event) {\n    // Get the title of the clicked button\n    const btnName = event.target.getAttribute(\"data-value\");\n    if (btnName === \"next\") {\n      const newUserIndex = userIndex + 1;\n      nextUser(newUserIndex);\n    } else {\n      const newUserIndex = userIndex - 1;\n      previousUser(newUserIndex);\n    }\n  }\n\n  function loadUsers() {\n    API.fetchUsers()\n      .then(users => {\n        setUsers(users);\n        setUser(users[0]);\n      })\n\n      .catch(err => console.log(err));\n  }\n\n  return (\n    <div>\n      <h1 className=\"text-center\">Welcome to LinkedUp</h1>\n      <p className=\"text-center h3\">Click on the arrows to browse users</p>\n      <Row>\n        {/* Pass props to the card container */}\n        {/* <CardContainer user={user}/> */}\n        {/* OR */}\n        <CardContainer {...user}/>\n      </Row>\n    </div>\n  );\n}\n\nexport default Gallery;\n","C:\\Users\\Elyse\\gt-pt\\sandbox\\react\\practicereactapp\\practicereactapp\\src\\components\\Navbar\\index.js",[],"C:\\Users\\Elyse\\gt-pt\\sandbox\\react\\practicereactapp\\practicereactapp\\src\\components\\Footer\\index.js",[],"C:\\Users\\Elyse\\gt-pt\\sandbox\\react\\practicereactapp\\practicereactapp\\src\\components\\Wrapper\\index.js",[],"C:\\Users\\Elyse\\gt-pt\\sandbox\\react\\practicereactapp\\practicereactapp\\src\\utils\\API.js",["72"],"import axios from \"axios\";\n\n// Export an object containing methods we'll use for accessing the random user API\nexport default {\n  fetchUsers: function() {\n    return axios\n      .get(\"https://api.github.com/orgs/github/public_members\")\n      .then(res => {\n        const users = res.data;\n        return users.map(user => {\n          return {\n            login: user.login,\n            image: user.avatar_url,\n            profileUrl: user.html_url\n          };\n        });\n      });\n  }\n};\n","C:\\Users\\Elyse\\gt-pt\\sandbox\\react\\practicereactapp\\practicereactapp\\src\\components\\CardContainer\\index.js",[],"C:\\Users\\Elyse\\gt-pt\\sandbox\\react\\practicereactapp\\practicereactapp\\src\\components\\Row\\index.js",[],"C:\\Users\\Elyse\\gt-pt\\sandbox\\react\\practicereactapp\\practicereactapp\\src\\components\\Card\\index.js",[],"C:\\Users\\Elyse\\gt-pt\\sandbox\\react\\practicereactapp\\practicereactapp\\src\\components\\CardBtn\\index.js",[],"C:\\Users\\Elyse\\gt-pt\\sandbox\\react\\practicereactapp\\practicereactapp\\src\\components\\CardBody\\index.js",[],"C:\\Users\\Elyse\\gt-pt\\sandbox\\react\\practicereactapp\\practicereactapp\\src\\components\\CardHeading\\index.js",[],"C:\\Users\\Elyse\\gt-pt\\sandbox\\react\\practicereactapp\\practicereactapp\\src\\components\\CardImage\\index.js",[],"C:\\Users\\Elyse\\gt-pt\\sandbox\\react\\practicereactapp\\practicereactapp\\src\\components\\CardTitle\\index.js",[],"C:\\Users\\Elyse\\gt-pt\\sandbox\\react\\practicereactapp\\practicereactapp\\src\\components\\CardTitleText\\index.js",[],{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","severity":1,"message":"78","line":34,"column":12,"nodeType":"79","messageId":"80","endLine":34,"endColumn":26},{"ruleId":"81","severity":1,"message":"82","line":4,"column":1,"nodeType":"83","endLine":19,"endColumn":3},"no-native-reassign",["84"],"no-negated-in-lhs",["85"],"no-unused-vars","'handleBtnClick' is defined but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Assign object to a variable before exporting as module default","ExportDefaultDeclaration","no-global-assign",